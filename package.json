{
    "name": "@harrybin/react-common",
    "version": "0.0.1",
    "description": "A library that contains useful generic react components helperfunctions and custom hooks.",
    "author": "harrybin",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/harrybin/react-common.git"
    },
    "homepage": "https://pages.github.com/harrybin/react-common",
    "main": "dist/cjs/index.js",
    "module": "dist/esm/index.js",
    "types": "dist/esm/types/src/index.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "predeploy": "npm run build && npm run docs && npm run build-storybook",
        "deploy": "gh-pages -d storybook-static",
        "docs": "npx typedoc",
        "build": "rollup -c",
        "storybook": "storybook dev -p 6006",
        "build-storybook": "storybook build",
        "test": "vitest run",
        "test:ci": "vitest run",
        "test:watch": "vitest watch",
        "test:ui": "vitest --ui --api 3333 --coverage",
        "test:coverage": "vitest run --coverage",
        "release": "standard-version --skip-commit"
    },
    "publishConfig": {
        "@harrybin:registry": "https://guthub.com/harrybin/"
    },
    "dependencies": {
        "@emotion/css": "^11.11.2",
        "@emotion/react": "^11.11.1",
        "@emotion/styled": "^11.11.0",
        "@mui/material": "^5.14.3",
        "@rollup/plugin-commonjs": "^25.0.3",
        "markdown-to-jsx": "^7.2.1"
    },
    "devDependencies": {
        "@rollup/plugin-json": "^6.0.0",
        "@rollup/plugin-node-resolve": "^15.1.0",
        "@rollup/plugin-typescript": "^11.1.2",
        "@storybook/addon-docs": "^7.2.0",
        "@storybook/addon-essentials": "^7.2.0",
        "@storybook/addon-interactions": "^7.2.0",
        "@storybook/addon-links": "^7.2.0",
        "@storybook/addon-mdx-gfm": "^7.2.0",
        "@storybook/blocks": "^7.2.0",
        "@storybook/react": "^7.2.0",
        "@storybook/react-vite": "^7.2.0",
        "@storybook/testing-library": "^0.2.0",
        "@mui/icons-material": "^5.14.3",
        "@testing-library/jest-dom": "^5.17.0",
        "@testing-library/react": "^14.0.0",
        "@types/react": "^18.2.18",
        "@vitest/coverage-v8": "^0.34.1",
        "@vitest/ui": "^0.34.1",
        "identity-obj-proxy": "^3.0.0",
        "jsdom": "^22.1.0",
        "prop-types": "^15.8.1",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "rimraf": "^5.0.1",
        "rollup": "^3.27.0",
        "rollup-plugin-dts": "^5.3.1",
        "rollup-plugin-replace": "^2.2.0",
        "standard-version": "^9.5.0",
        "storybook": "^7.2.0",
        "tslib": "^2.6.1",
        "tss-react": "^4.8.8",
        "typedoc": "^0.24.8",
        "typescript": "^5.1.6",
        "unplugin-auto-import": "^0.16.6",
        "vitest": "^0.34.1",
        "vitest-fetch-mock": "^0.2.2",
        "vitest-sonar-reporter": "^0.4.1"
    },
    "peerDependencies": {
        "@mui/icons-material": "^5.14.3",
        "react": "18.2.0"
    }
}